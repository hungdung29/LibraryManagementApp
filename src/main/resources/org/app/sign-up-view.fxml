<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="436.0" prefWidth="473.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.app.Controller.SignUpViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

   <Label fx:id="messageLabel" text="Sign Up" />
    <TextField fx:id="usernameText" />
    <PasswordField fx:id="pwText" />
    <PasswordField fx:id="cfPwText" />
    <Button fx:id="signUpButton" mnemonicParsing="false" onAction="#onSignUpButtonClicked" text="Sign Up" />
   <Button fx:id="backButton" mnemonicParsing="false" onAction="#onBackButtonClicked" text="Back" />
</VBox>
